{
  "name": "react-input-field",
  "version": "1.2.0",
  "description": "React Input Field",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zippyui/react-input-field.git"
  },
  "scripts": {
